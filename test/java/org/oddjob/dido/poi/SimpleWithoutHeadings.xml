<dido-poi:book xmlns:dido-poi="oddjob:dido:poi">
	<styles>
		<dido-poi:style key="date" format="m/d/yy hh:mm" />
		<dido-poi:style key="percentage" format="#0.00%" />
	</styles>
	<of>
		<dido-poi:sheet>
			<of>
				<dido-poi:rows name="person" autoWidth="true">
					<of>
						<dido-poi:text name="name" title="Name" />
						<dido-poi:date name="dateOfBirth" title="Date Of Birth" style="date" />
						<dido-poi:numeric name="salary" title="Salary"/>
						<dido-poi:numeric-formula name="bonus" title="Bonus"
							formula="10" />
						<dido-poi:numeric name="percentage" title="Percentage"
								style="percentage" value='0.1'/>
					</of>
				</dido-poi:rows>
			</of>
		</dido-poi:sheet>
	</of>
</dido-poi:book>
