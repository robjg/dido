<oddjob id="this" xmlns:dido='oddjob:dido'>
 <job>
  <dido:read id='read'>
   <plan>
    <import resource="org/oddjob/dido/DelimitedExamplePlan.xml"/>
   </plan>
   <bindings>
    <dido:bean-binding key='person'>
     <type>
      <class name='org.oddjob.dido.Person'/>
     </type>
    </dido:bean-binding>
   </bindings>
   <input>
    <resource resource="org/oddjob/dido/ReadJobExampleData.txt"/>
   </input>
   <beans>
    <list/>
   </beans>
  </dido:read>
 </job>
</oddjob>

