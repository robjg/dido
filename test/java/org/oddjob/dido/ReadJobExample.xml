<oddjob id="this" xmlns:dido='http://rgordon.co.uk/oddjob/dido'>
 <job>
  <dido:read id='read'>
   <plan>
    <dido:plan>
     <configuration>
      <arooa:configuration xmlns:arooa='http://rgordon.co.uk/oddjob/arooa'
       resource="org/oddjob/dido/DelimitedExamplePlan.xml"/>
     </configuration>
    </dido:plan>
   </plan>
   <bindings>
    <dido:bean-binding node='person'>
     <type>
      <class name='org.oddjob.dido.Person'/>
     </type>
    </dido:bean-binding>
   </bindings>
   <input>
    <resource resource="org/oddjob/dido/ReadJobExampleData.txt"/>
   </input>
  </dido:read>
 </job>
</oddjob>

