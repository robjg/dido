<arooa:descriptor xmlns:arooa="http://rgordon.co.uk/oddjob/arooa"
        prefix="dido-poi" namespace="oddjob:dido:poi">
	<values>
		<arooa:bean-def element='rows'
		    className='dido.poi.layouts.DataRows'/>
		<arooa:bean-def element='boolean'
		    className='dido.poi.layouts.BooleanCell'/>
		<arooa:bean-def element='date'
		    className='dido.poi.layouts.DateCell'/>
		<arooa:bean-def element='numeric'
		    className='dido.poi.layouts.NumericCell'/>
		<arooa:bean-def element='text'
		    className='dido.poi.layouts.TextCell'/>
		<arooa:bean-def element='numeric-formula'
		    className='dido.poi.layouts.NumericFormulaCell'/>
		<arooa:bean-def element='text-formula'
		    className='dido.poi.layouts.TextFormulaCell'/>
		<arooa:bean-def element='blank'
		    className='dido.poi.layouts.BlankCell'/>
		<arooa:bean-def element='style'
		    className='dido.poi.style.StyleBean'/>
        <arooa:bean-def element='workbook'
            className='dido.poi.data.PoiWorkbook'/>
	</values>
</arooa:descriptor>